        "yii", "yii2", "bootstrap", "material"
    ],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/romdim/yii2-bootstrap-material/issues",
        "source": "https://github.com/romdim/yii2-bootstrap-material"
    },
    "authors": [
        {
            "name": "Marcelo de Andrade",
            "email": "andrade.asmarcelo@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "npm-asset/bootstrap-material-design": "0.5.10"
    },
    "autoload": {
        "psr-4": {
            "marcelodeandrade\\material\\": "src/"
        }
    }
